{
  "name": "__MSG_name__",
  "short_name": "__MSG_nameShort__",
  "author": "Original: Tumuyan; Maintainer: sperwe",
  "version": "4.9.8",
  "version_name": "4.9.8 by sperwe - Simplified Note List & Open Tab Feature",
  "update_url": "https://raw.githubusercontent.com/sperwe/Tree-Style-History/main/updates.xml",
  "manifest_version": 2,
  "minimum_chrome_version": "22.0",
  "default_locale": "en",
  "description": "v4.9.8: Simplified note list UI - Removed header layer for cleaner look. Added open in tab button. Fixed notes.html modal styles for light theme.",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/page-notes.js"],
      "css": ["css/page-notes.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "history",
    "bookmarks",
    "sessions",
    "chrome://favicon/",
    "contextMenus",
    "notifications"
  ],
  "icons": {
    "16": "images/tree-16.png",
    "32": "images/tree-32.png",
    "48": "images/tree-48.png",
    "128": "images/tree-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/tree-19.png",
      "38": "images/tree-38.png",
      "32": "images/tree-32.png",
      "48": "images/tree-48.png",
      "128": "images/tree-128.png"
    },
    "default_popup": "popup.html"
  },
  "commands": {
    "open_history2": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "windows": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description":"__MSG_treeOpen__"
    },
    "open_history1": {
        "suggested_key": {
          "default": "Alt+Shift+H",
          "windows": "Alt+Shift+H",
          "mac": "Alt+Shift+H"
        },
        "description": "__MSG_showAllHistory__"
      },
    "open_closed": {
        "suggested_key": {
          "default": "Alt+Shift+G",
          "windows": "Alt+Shift+G",
          "mac": "Alt+Shift+G"
        },
        "description": "__MSG_OpenClosedHistory__"
      },
    "open_bookmark": {
        "suggested_key": {
          "default": "Alt+Shift+O",
          "windows": "Alt+Shift+O",
          "mac": "Alt+Shift+O"
        },
        "description": "__MSG_OpenBookMark__"
    }
  }
}